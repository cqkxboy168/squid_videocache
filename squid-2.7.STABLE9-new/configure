    return 1;
  if (((long int) (sizeof (u_int16_t))) < 0)
    {
      long int i = longval ();
      if (i != ((long int) (sizeof (u_int16_t))))
	return 1;
      fprintf (f, "%ld", i);
    }
  else
    {
      unsigned long int i = ulongval ();
      if (i != ((long int) (sizeof (u_int16_t))))
	return 1;
      fprintf (f, "%lu", i);
    }
  /* Do not output a trailing newline, as this causes \r\n confusion
     on some platforms.  */
  return ferror (f) || fclose (f) != 0;

  ;
  return 0;
